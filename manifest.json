{
	"name": "Getting Started Example",
	"version": "1.0",
	"description": "Build an Extension!",
	"permissions": ["storage","activeTab","https://www.moralfoundations.org/"],
	"background": {
		"scripts": ["background.js", "dicts.js"],
		"persistent": false
	},

	 "content_scripts": [
	   {
	     "matches": ["https://*.independent.com/*"],
	     "js": ["content.js"]
	   }
	 ],

	"manifest_version": 2
}
